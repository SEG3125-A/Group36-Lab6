<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Survey Results</title>
</head>
<body>
  <h1>Survey Results</h1>
  <ul>
    <% surveyResults.forEach(entry => { %>
      <li>
        <strong>Name:</strong> <%= entry.firstName %> <%= entry.lastName %><br>
        <strong>Satisfaction:</strong> <%= entry.satisfaction %><br>
        <strong>Improvement:</strong> <%= entry.improvement %><br>
        <strong>Comment:</strong> <%= entry.comment %><br>
        <strong>Submission:</strong> <%= entry.submission %><br>
      </li>
    <% }); %>
  </ul>
</body>
</html>
